{
  "name": "@darksoil-studio/notes-zome",
  "version": "0.500.0",
  "scripts": {
    "start": "vite --clearScreen false --port $UI_PORT",
    "build": "pnpm lint && tsc && pnpm localize:build && pnpm analyze",
    "lint": "eslint src && prettier \"src/**/*.ts\" --check --ignore-path .gitignore",
    "format": "eslint src --fix && prettier \"src/**/*.ts\" --write --ignore-path .gitignore",
    "localize:extract": "lit-localize extract",
    "localize:build": "lit-localize build",
    "analyze": "cem analyze --litelement --exclude dist"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "locales",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darksoil-studio/notes-zome.git"
  },
  "exports": {
    ".": "./dist/index.js",
    "./dist/*": "./dist/*",
    "./locales/*": "./locales/*"
  },
  "dependencies": {
    "@darksoil-studio/automerge-collaborative-prosemirror": "^0.500.1",
    "@darksoil-studio/automerge-collaborative-sessions": "^0.500.1",
    "@darksoil-studio/collaborative-sessions-zome": "0.500.0-rev.ee6f452d329909811ec1d04e9e184246a69a866d",
    "@darksoil-studio/holochain-elements": "^0.500.0",
    "@darksoil-studio/holochain-signals": "^0.500.0",
    "@darksoil-studio/holochain-utils": "^0.500.0",
    "@darksoil-studio/profiles-provider": "^0.500.0",
    "@holochain/client": "^0.19.0",
    "@lit/context": "^1.0.0",
    "@lit/localize": "^0.12.0",
    "@mdi/js": "^7.2.0",
    "@msgpack/msgpack": "^2.8.0",
    "@shoelace-style/shoelace": "^2.11.0",
    "lit": "^3.0.0"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "^0.10.3",
    "@darksoil-studio/profiles-zome": "0.500.0-rev.b211bab52e30e8730c0878e121b9649680976907",
    "@lit/localize-tools": "^0.6.3",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "vite": "^4.0.0",
    "vite-plugin-wasm": "^3.4.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "customElements": "custom-elements.json",
  "type": "module"
}